function ag_analyze_log() { 
	glob=./*
	field=8
	count=50
	FIELD=${2:-$field}
	COUNT=${3:-$count}
	GLOB=${1:-$glob}
	grep '.' $GLOB | cut -sf $FIELD | sort | uniq -c | sort -nr | head -n $COUNT
}

