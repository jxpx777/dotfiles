function killhelper {
    echo "Killing 1Password 4 WebStore Helper"
    launchctl stop 2BUA8C4S2C.com.agilebits.onepassword4-helper
    launchctl remove 2BUA8C4S2C.com.agilebits.onepassword4-helper
    echo "."

    echo
    echo "Killing 1Password 4 AppStore Helper"
    launchctl stop 2BUA8C4S2C.com.agilebits.onepassword-osx-helper   
    launchctl remove 2BUA8C4S2C.com.agilebits.onepassword-osx-helper   
    echo "."

    echo
    echo "Killing 1Password 3 AppStore Helper"
    launchctl stop com.agilebits.onepassword-osx-helper
    launchctl remove com.agilebits.onepassword-osx-helper
    echo "."

    echo
    echo "Killing 1Password 3 WebStore Helper"
    launchctl stop ws.agile.1PasswordAgent
    launchctl remove ws.agile.1PasswordAgent
    echo "."
}