export DOTFILES="$HOME/Projects/dotfiles"
export EDITOR='vim'
export GIT_EDITOR='vim'
export BUNDLER_EDITOR='vim'
export HOMEBREW_CASK_OPTS="--appdir=/Applications"
export NODE_PATH="/usr/local/lib/node"

export GOPATH="/usr/local/go:$HOME/Projects/AG/B5/vendor:$HOME/Projects/AG/B5/server"

export PATH=$GOPATH/bin:$HOME/Projects/AG/B5/bin:$HOME/bin:$HOME/bin/Multimarkdown/bin:$HOME/bin/abbot/bin:/usr/bin:opt/local/bin:/opt/local/sbin:/bin:/sbin:$PATH

export GIT_CONFIG_NOSYSTEM=1
if [[ ! -L $HOME/.gitconfig ]] then
	ln -nsf "$DOTFILES/git/gitconfig" $HOME/.gitconfig
fi
