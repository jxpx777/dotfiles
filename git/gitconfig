[diff]
	tool = Kaleidoscope

[merge]
	tool = Kaleidoscope
[mergetool "Changes"]
       cmd = /usr/bin/env chdiff --wait "$LOCAL" "$REMOTE"
       keepBackup = false
       trustExitCode = false
[mergetool "Kaleidoscope"]
	cmd = ksdiff --merge --output \"$MERGED\" --base \"$BASE\" -- \"$LOCAL\" --snapshot \"$REMOTE\" --snapshot
	trustExitCode = true
[mergetool]
	prompt = false
